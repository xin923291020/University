<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="model.Taxpayer" table="taxpayer" schema="dbo"
		catalog="VatInvoiceStatistics">
		<id name="taxpayerNumber" type="java.lang.String">
			<column name="taxpayerNumber" length="20" />
			<generator class="assigned" />
		</id>
		<property name="idNumber" type="java.lang.String">
			<column name="idNumber" length="20" />
		</property>
		<property name="taxpayerName" type="java.lang.String">
			<column name="taxpayerName" length="20" />
		</property>
		<property name="address" type="java.lang.String">
			<column name="address" length="40" />
		</property>
		<property name="phone" type="java.lang.String">
			<column name="phone" length="20" />
		</property>
	</class>
</hibernate-mapping>
